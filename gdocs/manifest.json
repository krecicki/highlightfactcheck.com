{
  "manifest_version": 3,
  "name": "Highlight Fact Check for Google Docs",
  "version": "0.1",
  "description": "Fact check any text on Google Docs sentences.",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "https://docs.google.com/*",
    "https://highlightfactcheck.com/*"
  ],
  "icons": {
    "16": "ico/icon.png",
    "48": "ico/icon.png",
    "128": "ico/icon.png"
  }
}